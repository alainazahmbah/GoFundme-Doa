# Reown (WalletConnect) Project ID
# Get this from https://cloud.reown.com
VITE_REOWN_PROJECT_ID=your_project_id_here

# IPFS Configuration
# For Infura IPFS, get credentials from https://infura.io
VITE_IPFS_AUTH=Basic_your_base64_encoded_credentials

# Smart Contract Addresses (deploy these first)
VITE_CAMPAIGN_FACTORY_ADDRESS=0x...

# Network Configuration
VITE_CHAIN_ID=1
VITE_NETWORK_NAME=mainnet